<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bugs.LoginController">
   <children>
      <TextField fx:id="usernameText" layoutX="168.0" layoutY="230.0" prefHeight="14.0" prefWidth="263.0" promptText="username" />
      <PasswordField fx:id="passwordText" layoutX="168.0" layoutY="265.0" prefHeight="26.0" prefWidth="263.0" promptText="password" />
      <Button fx:id="loginButton" layoutX="168.0" layoutY="305.0" mnemonicParsing="false" onAction="#onLoginButtonPressed" prefHeight="26.0" prefWidth="263.0" text="Login" />
      <HBox layoutX="168.0" layoutY="193.0" prefHeight="26.0" prefWidth="263.0">
         <children>
            <RadioButton fx:id="programmer" mnemonicParsing="false" prefHeight="38.0" prefWidth="137.0" text="Programmer" />
            <RadioButton fx:id="tester" mnemonicParsing="false" prefHeight="38.0" prefWidth="137.0" text="Tester" />
         </children>
      </HBox>
   </children>
</AnchorPane>
